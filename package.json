{
  "name": "data-poa",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration staging",
    "build:prod": "ng build --configuration=production",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "sonar": "ng test --no-watch --code-coverage && sonar-scanner",
    "cypress": "concurrently \"ng serve\" \"cypress open\"",
    "cypress:open": "cypress open",
    "cypress:chrome": "cypress run -b chrome",
    "dev:ssr": "ng run data-poa:serve-ssr",
    "serve:ssr": "node dist/data-poa/server/main.js",
    "build:ssr": "ng build && ng run data-poa:server",
    "build:ssr:stage": "ng build --configuration=staging && ng run data-poa:server:staging",
    "prerender": "ng run data-poa:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/platform-server": "~13.3.11",
    "@angular/router": "~13.3.11",
    "@ngrx/component-store": "^13.2.0",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/schematics": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "@nguniversal/express-engine": "^13.1.1",
    "cypress": "^9.4.1",
    "express": "^4.15.2",
    "jquery": "^3.6.0",
    "karma-sonarqube-unit-reporter": "0.0.23",
    "mapbox-gl": "^2.6.1",
    "ngx-cookie": "^5.0.1",
    "ngx-cookie-backend": "^5.0.1",
    "ngx-cookie-service": "^13.2.1",
    "node-sass": "^7.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "url": "^0.11.0",
    "xhr2": "^0.2.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.9",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "~13.3.9",
    "@angular/compiler-cli": "~13.3.11",
    "@nguniversal/builders": "^13.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/mapbox-gl": "^2.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "sonarqube-scanner": "^2.9.1",
    "typescript": "~4.6.2",
    "xhr2-cookies": "^1.1.0"
  }
}
